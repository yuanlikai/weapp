/* pages/me/me.wxss */

page {
  display: block;
  min-height: 100%;
  background-color: #f7f7f7;
}
.ulul{
  border: 1px solid red;
}
.head-img{
  float: left;
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx
}
.head{
  float: left;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 24rpx;
  margin-bottom: 24rpx;
  background: #fff;
}
.head-name{
  float: left;
  line-height: 140rpx;
  font-size: 36rpx;
  color: #333;
  background: #fff;
  margin-left: 36rpx;
}