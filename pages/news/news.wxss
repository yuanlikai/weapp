/* pages/news/news.wxss */
.news-banner{
  float: left;
  width: 100%;
  height: 213rpx;
  padding: 34rpx 24rpx;
  box-sizing: border-box
}
.news-banner-img{
  float: left;
  width: 100%;
  height: 100%;
}
.news-ul{
  float: left;
  width: 100%;
}
.news-ul-li{
  float: left;
  box-sizing: border-box;
  width: 702rpx;
  padding: 34rpx 0;
  margin-left: 24rpx;
  border-top: 1px solid rgba(247,247,247,1);
}
.news-ul-li-rig{
  float: right;
  width: 192rpx;
  height: 150rpx;
}
.news-ul-li-head{
  float: left;
  width: 450rpx;
  font-size: 32rpx;
  height: 78rpx;
  line-height: 40rpx;
  overflow: hidden;
  color: #444444;
}
.news-ul-big{
  float: left;
  width: 450rpx;
}
.news-ul-li-detai{
  float: left;
  margin-top: 25rpx;
  width: 450rpx;
  line-height: 40rpx;
}
.news-ul-li-head-img{
  float: left;
  width: 40rpx;
  height: 40rpx;
  border-radius: 20rpx;
}
.news-ul-li-nick{
  float: left;
  margin-left: 18rpx;
  font-size: 26rpx;
  color: #666666
}